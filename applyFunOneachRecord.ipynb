    "\n",
    "import re\n",
    "import pandas as pd\n",
    "strike_Info = pd.DataFrame()\n",
    "\n",
    "\n",
    "\n",
    "## Use Apply\n",
    "\n",
    "def extractStr(row):\n",
    "    return re.findall('\\D+', row)\n",
    "    \n",
    "strike_Info['name'] = strike_Info['Symbol'].apply(extractStr).values[0][0]\n",
    "strike_Info"
   ]
