   "source": [
    "from functools import cache"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 91,
   "metadata": {},
   "outputs": [],
   "source": [
    "@cache\n",
    "def fibo(n):\n",
    "    if n < 2:\n",
    "        return 2\n",
    "    return fibo(n-1)+fibo(n-2)\n",
    "\n"
   ]
  },
