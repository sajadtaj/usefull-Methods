    "import pandas as pd\n",
    " \n",
    "# change in date time format\n",
    "date_sr = pd.to_datetime(pd.Series(\"2012-09-02\"))\n",
    "change_format = date_sr.dt.strftime('%d-%m-%Y')\n",
    " \n",
    "# Print the formatted date\n",
    "print(change_format)"
